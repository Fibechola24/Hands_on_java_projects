package telusko;

public class Main {

    public static void main(String[] args) {
	// iterating through an array
        int [][] data = {
                {4,5,6,4,10},
                {3,6,7,8,9,0,9,6},
                {6,7,87}
        };
        for (int i = 0; i < data.length; i++){
            for (int j = 0; j < data[i].length; j++){
                System.out.print(data[i][j] + " ");
            }
            System.out.println();
        }
    }
}
